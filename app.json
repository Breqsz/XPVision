{
  "expo": {
    "name": "XP Vision",
    "slug": "xp-vision-app",
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "xpvision",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0B0B0B"
    },
    "platforms": ["ios", "android", "web"],
    "jsEngine": "hermes",
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.breq.xpvision"
    },
    "android": {
      "package": "com.breq.xpvision",
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0B0B0B"
      },
      "splash": {
        "image": "./assets/splash.png",
        "resizeMode": "contain",
        "backgroundColor": "#0B0B0B"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
